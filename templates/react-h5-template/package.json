    "build": "father-build",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "npx standard-version --no-verify -t release-"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/react": "^17.0.33",
